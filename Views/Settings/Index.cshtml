@model IEnumerable<Salon.Models.Settings>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SettingID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SettingValue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SettingID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SettingValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SettingID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SettingID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SettingID })
        </td>
    </tr>
}

</table>
